{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["es2020"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "parseGithubUrl/dist",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*", "parseGithubUrl/src/types/*"]
